{
  "name": "@hello-world-co-op/dao-admin-suite",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "DAO admin dashboard - authenticated admin area for KYC review, member management, system monitoring, and governance oversight",
  "scripts": {
    "dev": "vite",
    "dev:qa": "VITE_DEV_AUTH_BYPASS=true vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "deploy": "dfx deploy"
  },
  "dependencies": {
    "@dfinity/principal": "^3.4.3",
    "@hello-world-co-op/api": "file:../api",
    "@hello-world-co-op/auth": "file:../auth",
    "@hello-world-co-op/ui": "file:../ui",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "globals": "^17.1.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.53.1",
    "vite": "^5.4.0",
    "vitest": "^1.6.1"
  }
}
